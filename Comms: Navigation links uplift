SELECT url, keyword,date, case when date between '2019-10-09' and '2019-10-23' then 'Pre-period' when date between '2019-10-24' and '2019-11-08' then 'Post-period' else "" end as period,
sum(impressions) as impressions, sum(clicks) as clicks, avg(position)as avg_pos     FROM `rvu-seo.search_console_eu.eu_uswitch_page_query_country_device` 

where date between '2019-10-09' and '2019-11-08'
AND url in ('https://www.uswitch.com/broadband/compare/home_phone_and_landline/','https://www.uswitch.com/broadband/wireless-broadband/','https://www.uswitch.com/broadband/cheap-broadband/','https://www.uswitch.com/broadband/broadband-and-free-line-rental/',
'https://www.uswitch.com/broadband/providers/talktalk/','https://www.uswitch.com/broadband/providers/talktalk/','https://www.uswitch.com/broadband/providers/sky/','https://www.uswitch.com/broadband/providers/plusnet/','https://www.uswitch.com/broadband/providers/virgin_media/','https://www.uswitch.com/broadband/providers/vodafone/',
'https://www.uswitch.com/broadband/providers/ee/','https://www.uswitch.com/broadband/providers/now_broadband/','https://www.uswitch.com/broadband/broadband-only/','https://www.uswitch.com/broadband/providers/bt/')
AND country = 'gbr'
AND keyword in ('best broadband and phone deals including line rental',
'broadband and line rental deals',
